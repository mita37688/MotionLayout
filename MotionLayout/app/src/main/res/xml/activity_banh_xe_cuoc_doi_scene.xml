<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="5000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/imageView4" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="-360" />
        <Constraint
            motion:layout_editor_absoluteY="473dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="274dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView9" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="360"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            motion:layout_editor_absoluteY="454dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="118dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView5" />
        <Constraint
            motion:layout_editor_absoluteY="583dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="57dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView7" />
        <Constraint
            motion:layout_editor_absoluteY="461dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="283dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView9" />
    </ConstraintSet>
</MotionScene>